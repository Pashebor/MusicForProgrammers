<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="fontType" format="enum">
        <enum name="regular" value="0" />
        <enum name="italic" value="1" />
    </attr>
    <attr name="buttonType" format="enum">
        <enum name="grey" value="0" />
        <enum name="purple" value="1" />
        <enum name="fuschia" value="2" />
        <enum name="blue" value="3" />
        <enum name="green" value="4" />
        <enum name="orange" value="5" />
        <enum name="yellow" value="6" />
    </attr>
    <declare-styleable name="MFPTextView">
        <attr name="fontType" />
    </declare-styleable>
    <declare-styleable name="MFPButton">
        <attr name="buttonType" />
    </declare-styleable>
</resources>